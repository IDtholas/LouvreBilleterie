<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>Accueil de la Billeterie</title>
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
</head>
<body>
<div class="container">
    <div id="header" class="jumbotron">
        <h1 class="text-center"> Billeterie du mus√©e du Louvre</h1>
    </div>

    <div>
        {{ form_start(form) }}

        {{ form_label(form.nom) }}
        {{ form_widget(form.nom) }}
        {{ form_errors(form.nom) }}
        {{ form_label(form.prenom) }}
        {{ form_widget(form.prenom) }}
        {{ form_errors(form.prenom) }}
        {{ form_errors(form.email) }}
        {{ form_label(form.email) }}
        {{ form_widget(form.email) }}
        {{ form_errors(form.dateDeVisite) }}
        {{ form_label(form.dateDeVisite) }}
        {{ form_widget(form.dateDeVisite) }}
        {{ form_errors(form.typeTicket) }}
        {{ form_label(form.typeTicket) }}
        {{ form_widget(form.typeTicket) }}
        <br />
        <button type="submit" class="btn btn-default" >Commencer la commande </button>
        {{ form_end(form) }}
    </div>
</div>
</body>